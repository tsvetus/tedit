{
  "name": "tedit",
  "version": "1.1.1",
  "description": "Set of visual React components",
  "main": "./index.js",
  "scripts": {
    "watch": "webpack --watch --config webpack.config.watch.js",
    "dev": "webpack-dev-server --color --hot --inline --watch --config webpack.config.dev.js",
    "exam": "webpack --display-error-details --config webpack.config.exam.js",
    "prod": "webpack --display-error-details --config webpack.config.prod.js",
    "doc": "jsdoc src README.md --verbose -r -d docs/jsdoc",
    "build": "webpack --config webpack.config.prod.js && webpack --config webpack.config.exam.js && jsdoc src/index.js --destination doc"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tsvetus/tedit.git"
  },
  "keywords": [
    "input",
    "ReactJS",
    "component"
  ],
  "author": "Vyacheslav Tsvetus",
  "email": "tsvetus@gmail.com",
  "license": "MIT",
  "dependencies": {
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-react": "^7.7.0",
    "babel-loader": "^8.0.6",
    "jsdoc": "^3.6.3",
    "path": "^0.12.7",
    "webpack": "^4.41.2",
    "webpack-dev-server": "^3.9.0"
  }
}
